{
    "extends": [
        "@commitlint/config-conventional"
    ],
    "rules": {
        "body-leading-blank": [
            0,
            "always"
        ],
        "body-max-line-length": [
            2,
            "always",
            200
        ],
        "header-max-length": [
            2,
            "always",
            200
        ]
    }
}
